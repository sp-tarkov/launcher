@page "/AdminPage"

@inject LogHelper _logHelper

<MudContainer Class="px-8 my-2 pa-0" MaxWidth="MaxWidth.Large">
    <MudPaper Class="mud-width-full pa-4 flex-column">
        @foreach (var log in _logHelper.GetLogs())
        {
            <MudText Typo="Typo.body1">
                @log
            </MudText>
        }
    </MudPaper>
</MudContainer>
